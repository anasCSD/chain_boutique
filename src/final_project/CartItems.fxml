        </columns>
      </TableView>
      <Button layoutX="15.0" layoutY="313.0" mnemonicParsing="false" onAction="#removeItemsOnClick" text="Remove Items" />
      <Button layoutX="433.0" layoutY="369.0" mnemonicParsing="false" onAction="#proceedOrderOnclick" prefHeight="31.0" prefWidth="156.0" text="Proceed to CheckOut" />
      <Text layoutX="23.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cart Item List : " />
      <Label layoutX="293.0" layoutY="318.0" text="Total Price :" />
